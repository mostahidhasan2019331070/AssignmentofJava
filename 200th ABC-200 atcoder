import java.util.Arrays;
import java.util.Scanner;

public class Myclass {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int y=sc.nextInt();
        for(int i=0;i<y;i++)
        {

            if(x%200==0)
            {
                x/=200;
            }
            else {
                x=(x*1000+200);
            }
        }
System.out.println(x);
    }
}
